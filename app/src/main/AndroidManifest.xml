<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.prgr.quizards.canary">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_logo"
        android:label="Quizards"
        android:roundIcon="@mipmap/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizardsAlpha"
        tools:replace="android:theme, android:roundIcon, android:icon, android:label "
        tools:targetApi="31">
        <activity
            android:name=".end"
            android:exported="false" />
        <activity
            android:name=".answerscrn"
            android:exported="false" />
        <activity
            android:name=".question"
            android:exported="false" />
        <activity
            android:name=".inputdisp"
            android:exported="false" />
        <activity
            android:name=".activity_home_screen"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>